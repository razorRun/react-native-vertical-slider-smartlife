{
  "name": "react-native-vertical-slider-smartlife",
  "version": "1.0.6",
  "description": "A vertical Slider for React Native written entirely in javascript.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "rn-vertical-slider.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "dotenv release-it --dry-run",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "vertical-slider",
    "column-slider",
    "rn",
    "rn-vertical-slider",
    "react-native-vertical-slider-smartlife"
  ],
  "repository": "https://github.com/razorRun/react-native-vertical-slider-smartlife",
  "author": "rmilinda@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/razorRun/react-native-vertical-slider-smartlife/issues"
  },
  "homepage": "https://github.com/razorRun/react-native-vertical-slider-smartlife#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "^0.16.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "commitlint": "^8.3.5",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "release-it": "^13.5.8",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
}
